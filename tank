length= int(input("Enter the length of the tank."))
width= int(input("Enter the width of the tank."))
height= int(input("Enter the height of the tank."))

volume= (length * width * height)/1000 
gallons= (volume / 4.546)
gallons=round(gallons)
print("A", length, "x", width, "x", height, "cm tank is", volume, "litres and", gallons, "gallons.")
